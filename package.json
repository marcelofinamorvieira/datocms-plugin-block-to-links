{
  "name": "datocms-plugin-block-to-links",
  "description": "A plugin that allows you to convert blocks to links",
  "homepage": "https://github.com/marcelofinamorvieira/datocms-plugin-block-to-links",
  "private": false,
  "version": "0.2.0",
  "author": "Marcelo Finamor Vieira <marcelofinamorvieira@gmail.com>",
  "type": "module",
  "keywords": [
    "datocms-plugin"
